<div class="main-container col1-layout">
  <div class="container">
    <div class="row">

      <!-- Home Tabs  -->
      <div hidden class="current_time" content="{{current_time}}"></div>
      <div class="col-sm-12 col-md-12 col-xs-12">
        <div class="home-tab">
          <ul class="nav home-nav-tabs home-product-tabs">
            <li class="active">
              <a href="#featured" data-toggle="tab" aria-expanded="false">Sắp kết thúc phiên đấu giá</a>
            </li>

          </ul>
          <div id="productTabContent" class="tab-content">
            <div class="tab-pane active in" id="featured">
              <div class="featured-pro">
                <div class="slider-items-products">
                  <div id="featured-slider" class="product-flexslider hidden-buttons">
                    <div class="slider-items slider-width-col4">
                      {{#each this.topFiveDeadline}}
                      <div class="product-item">
                        <div class="item-inner">
                          <div class="product-thumbnail">
                            {{#if is_new}}
                            <div class="icon-hot-label hot-right">NEW</div>
                            {{/if}}
                            <div class="pr-img-area"> <a title="Ipsums Dolors Untra" href="/products/{{id}}">
                                <figure> <img class="first-img" src="/images/products-images/{{image}}"
                                    alt="html template"> <img class="hover-img" src="/images/products-images/{{image}}"
                                    alt="html template"></figure>
                              </a>

                            </div>
                            <div class="jtv-box-timer">
                              <div class="countbox_1 jtv-timer-grid count_{{id}}" value="count_{{id}}"
                                timerun="{{f_expired_at}}" style="margin-bottom: 50px;">
                                <div class="day box-time-date"><span class="time-num time-day">00</span>Days</div>
                                <div class="hour box-time-date"><span class="time-num">00</span>Hrs</div>
                                <div class="min box-time-date"><span class="time-num">00</span>Mins</div>
                                <div class="sec box-time-date"><span class="time-num">00</span>Secs</div>
                              </div>
                            </div>
                            <div class="pr-info-area">
                              <div class="pr-button">
                                <div class="mt-button add_to_compare" style="visibility: hidden;"> <a
                                    href="compare.html"> <i class="fa fa-signal"></i>
                                  </a> </div>
                                <div class="mt-button add_to_wishlist wish_{{id}}" data-id="{{id}}"> <a
                                    href="javascript:;"> <i class="fa fa-heart"></i>
                                  </a> </div>

                              </div>
                            </div>
                          </div>
                          <div class="item-info">
                            <div class="info-inner">
                              <div class="item-title"> <a title="Ipsums Dolors Untra" href="/products/{{id}}"> {{p_name}} </a> </div>
                              <div class="item-content">

                                <div class="item-price">
                                  <div class="price-box"> <span class="regular-price"><label>Giá cao nhất: </label>
                                      <span class="price">{{format current_bid}}</span>
                                    </span> </div>
                                </div>
                              </div>
                              <div class="item-brief" style="text-align: left;">
